{
    "name": "creocoder/yii2-flysystem",
    "description": "The flysystem extension for the Yii framework",
    "keywords": [
        "yii2",
        "flysystem",
        "filesystem",
        "files",
        "aws",
        "s3",
        "azure",
        "dropbox",
        "ftp",
        "gridfs",
        "sftp",
        "webdav"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexander Kochetov",
            "email": "creocoder@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "yiisoft/yii2": "^2.0",
        "league/flysystem": "^3.0"
    },
    "suggests": {
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-azure-blob-storage": "^3.0",
        "spatie/flysystem-dropbox": "^3.0",
        "league/flysystem-gridfs": "^3.0",
        "league/flysystem-sftp-v3": "^3.0",
        "league/flysystem-webdav": "^3.0",
        "league/flysystem-ziparchive": "^3.0",
        "league/flysystem-google-cloud-storage": "^3.0",
        "league/flysystem-ftp": "^3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "creocoder\\flysystem\\": "src"
        }
    }
}
